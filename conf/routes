GET     /bng/player-search/:platform/:playerName    controllers.BungieApi.playerSearch(platform, playerName)
GET     /bng/inventory/:platform/:playerName        controllers.BungieApi.inventory(platform, playerName)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                               controllers.Assets.versioned(path="/public", file: Asset)
# app bootstrap
GET     /$path<.*>                                  controllers.Application.index(path)
